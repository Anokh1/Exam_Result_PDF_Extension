{
    "manifest_version":2,
    "version":"1.0",
    "name":"Exam_Result_PDF_Extension",
    "icons": {
        "64": "icons/icon16.png"
    },
    "browser_action":{
        "default_icon":{
          "19":"icons/icon16.png",
          "38":"icons/icon16.png"
        }
    },
    "content_scripts":[
      {
       "matches":["<all_urls>"],
       "js":["main.js"]
      }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "permissions":[
        "tabs"
    ]
  }
  